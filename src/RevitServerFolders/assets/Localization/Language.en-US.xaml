<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String
        x:Key="RvtFromRsWindow.Title">Export RVT files from RS</system:String>
    <system:String
        x:Key="NwcFromRvtWindow.Title">Export Navisworks View</system:String>
    <system:String
        x:Key="NwcFromRvtWindow.PluginNotInstalled">Navisworks NWC Export Utility plugin is missing. You need to install it.</system:String>

    <system:String
        x:Key="Transaction.ChangeSite">Change site</system:String>

    <system:String
        x:Key="Exceptions.ServerNotFound">Server "{0}" was not found.</system:String>
    <system:String
        x:Key="Exceptions.FolderNotFound">Folder "{0}" was not found.</system:String>
    <system:String
        x:Key="Exceptions.ViewWithoutElements">View "{0}" does not contain any elements with geometry. It's possible that all relationships have been unloaded from the document. It's recommended to open the exported rvt file with all open workspaces and verify that the view contains elements from the active file.</system:String>
    <system:String
        x:Key="Exceptions.ViewNotFound">View not found "{0}".</system:String>
    <system:String
        x:Key="Exceptions.NwcExportError">NWC export error. Error message: {0}.</system:String>
    <system:String
        x:Key="Exceptions.NwcExportCancel">Revit canceled NWC export.</system:String>
    <system:String
        x:Key="Exceptions.RvtExportError">RVT export error. Error message: {0}.</system:String>
    <system:String
        x:Key="Exceptions.ElementsNotDeleted">While opening the document, elements causing errors were detected. These could not be automatically removed due to insufficient permissions to delete them. Revit canceled the export operation. The IDs of the elements causing the errors are: {0}. Error information: {1}.</system:String>

    <system:String
        x:Key="ErrorsWindow.ErrorGroupHeader">Export {0}</system:String>
    <system:String
        x:Key="ErrorsWindow.Title">Export errors</system:String>

    <system:String
        x:Key="MainWindow.Validation.SelectTargetFolder">Select a target folder.</system:String>
    <system:String
        x:Key="MainWindow.Validation.TargetFolderNotExist">Select an existing target folder.</system:String>
    <system:String
        x:Key="MainWindow.Validation.SelectSourceFolder">Select a source folder.</system:String>
    <system:String
        x:Key="MainWindow.Validation.SourceFolderEmpty">No models in the source folder.</system:String>
    <system:String
        x:Key="MainWindow.Validation.AllModelsSkipped">All models are marked as skipped.</system:String>
    <system:String
        x:Key="MainWindow.Validation.Wait">Wait for the loading to finish</system:String>
    <system:String
        x:Key="MainWindow.Validation.ModelsDuplicated">The source folder contains duplicates "{0}".</system:String>
    <system:String
        x:Key="MainWindow.Validation.AllSettingsOff">No settings selected</system:String>

    <system:String
        x:Key="RsBrowser.Validation.SelectFolder">Select a folder with models</system:String>

    <system:String
        x:Key="MainWindow.LabelTarget">Target folder</system:String>
    <system:String
        x:Key="MainWindow.LabelSource">Source folder</system:String>
    <system:String
        x:Key="MainWindow.ClearTarget">Clear target folder</system:String>
    <system:String
        x:Key="MainWindow.ExportRooms">Export rooms</system:String>
    <system:String
        x:Key="MainWindow.OpenTargetWhenFinish">Open target folder after completion</system:String>
    <system:String
        x:Key="MainWindow.NameHeader">Name</system:String>
    <system:String
        x:Key="MainWindow.FullNameHeader">Full name</system:String>
    <system:String
        x:Key="MainWindow.TargetsHeader">Export settings</system:String>
    <system:String
        x:Key="MainWindow.TargetsFrom">From:</system:String>
    <system:String
        x:Key="MainWindow.TargetsTo">To:</system:String>

    <system:String
        x:Key="MainWindow.ErrorTextPattern">Error in setting {0}: {1}</system:String>

    <system:String
        x:Key="RsBrowser.RefreshNode">Refresh node</system:String>
    <system:String
        x:Key="RsBrowser.Title">Select folder</system:String>
    <system:String
        x:Key="RsBrowser.NameHeader">Name</system:String>
    <system:String
        x:Key="RsBrowser.SizeHeader">Size</system:String>

    <system:String
        x:Key="Ok">OK</system:String>
    <system:String
        x:Key="Cancel">Cancel</system:String>

</ResourceDictionary>
