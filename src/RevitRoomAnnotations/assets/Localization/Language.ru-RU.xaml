<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String
        x:Key="MainWindow.Title">Обновить помещения в файле ЭОМ</system:String>
    <system:String
        x:Key="MainWindow.LinkName">Наименование связи</system:String>
    <system:String
        x:Key="MainWindow.LinkStatus">Статус связи</system:String>
    <system:String
        x:Key="MainWindow.ButtonOk">Обработать</system:String>
    <system:String
        x:Key="MainWindow.ButtonCancel">Отмена</system:String>
    <system:String
        x:Key="MainWindow.StatusTextLoaded">Загружена</system:String>
    <system:String
        x:Key="MainWindow.StatusTextUnloaded">Не загружена</system:String>    
    <system:String
        x:Key="MainViewModel.ErrorTextNoSelection">Связанные файлы не выбраны</system:String>
    <system:String
        x:Key="MainViewModel.ErrorTextWrongSelection">Некоторые выбранные связи не загружены</system:String>
    <system:String
        x:Key="MainViewModel.TransactionName">Обработка помещений</system:String>    
    <system:String
        x:Key="MainViewModel.Message.AnnotationsToCreate">Будет создано аннотаций: {0}</system:String>
