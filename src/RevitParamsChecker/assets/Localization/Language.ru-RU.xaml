<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String
        x:Key="MainWindow.Title">Проверка параметров</system:String>

    <system:String
        x:Key="DashboardPage.Title">Главная</system:String>
    <system:String
        x:Key="FiltrationPage.Title">Поисковые наборы</system:String>
    <system:String
        x:Key="RulesPage.Title">Правила</system:String>
    <system:String
        x:Key="ChecksPage.Title">Проверки</system:String>
    <system:String
        x:Key="ResultsPage.Title">Анализ</system:String>
    <system:String
        x:Key="Exceptions.ParamNotFound">Параметр отсутствует: {0}</system:String>

    <system:String
        x:Key="NamesSelectionDialog.NamesHeader">Название</system:String>

    <system:String
        x:Key="ChecksPage.NewRulePrompt">Новая проверка</system:String>
    <system:String
        x:Key="ChecksPage.RenameRulePrompt">Переименование проверки</system:String>

    <system:String
        x:Key="RulesPage.NewRulePrompt">Новое правило</system:String>
    <system:String
        x:Key="RulesPage.RenameRulePrompt">Переименование правила</system:String>

    <system:String
        x:Key="FiltersPage.NewFilterPrompt">Новый поисковый набор</system:String>
    <system:String
        x:Key="FiltersPage.RenameFilterPrompt">Переименование поискового набора</system:String>
    <system:String
        x:Key="FiltersPage.ShowFilterElements">Показать элементы</system:String>

    <system:String
        x:Key="PageMenu.Tooltip.Load">Загрузить</system:String>
    <system:String
        x:Key="PageMenu.Tooltip.Save">Сохранить</system:String>
    <system:String
        x:Key="PageMenu.Tooltip.SaveAs">Сохранить как</system:String>

    <system:String
        x:Key="EditableList.Tooltip.Add">Добавить</system:String>
    <system:String
        x:Key="EditableList.Tooltip.Rename">Переименовать</system:String>
    <system:String
        x:Key="EditableList.Tooltip.Copy">Копировать</system:String>
    <system:String
        x:Key="EditableList.Tooltip.Remove">Удалить</system:String>

    <system:String
        x:Key="NameEditor.Label">Имя:</system:String>
    <system:String
        x:Key="NameEditor.Validation.EmptyName">Введите новое название</system:String>
    <system:String
        x:Key="NameEditor.Validation.DuplicatedName">Такое название уже существует</system:String>

    <system:String
        x:Key="Ok">ОК</system:String>
    <system:String
        x:Key="Cancel">Отмена</system:String>
</ResourceDictionary>