<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String
        x:Key="Ok">OK</system:String>
    <system:String
        x:Key="Cancel">Cancel</system:String>

    <system:String
        x:Key="SettingsWindow.Title">Settings</system:String>

    <system:String
        x:Key="ElementVisibilitySettings.ColorLabel">Color:</system:String>
    <system:String
        x:Key="ElementVisibilitySettings.TransparencyLabel">Transparency:</system:String>

    <system:String
        x:Key="SettingsWindow.Validation.TransparancyLessThanZero">Transparency cannot be less than 0</system:String>
    <system:String
        x:Key="SettingsWindow.Validation.TransparancyGreaterThanHundred">Transparency cannot be more than 100%</system:String>
    <system:String
        x:Key="SettingsWindow.MainElementVisibilitySettings">Visibility settings for the first collision element</system:String>
    <system:String
        x:Key="SettingsWindow.SecondElementVisibilitySettings">Visibility settings for the second collision element</system:String>

    <system:String
        x:Key="Transactions.IsolateClashElements">Isolate clash elements</system:String>
    <system:String
        x:Key="Transactions.ClashGraphicSettings">Clash elements graphic settings</system:String>
    <system:String
        x:Key="Transactions.ClearFilters">Reset clash element filters</system:String>

    <system:String
        x:Key="Filters.FirstElementFilter">BIM_clashes_first_element_{0}</system:String>
    <system:String
        x:Key="Filters.SecondElementFilter">BIM_clashes_second_element_{0}</system:String>
    <system:String
        x:Key="Filters.NotFirstElementFilter">BIM_clashes_not_first_element_{0}</system:String>
    <system:String
        x:Key="Filters.NotSecondElementFilter">BIM_clashes_not_second_element_{0}</system:String>
    <system:String
        x:Key="Filters.NotElementsOfCollisionCategories">BIM_clashes_not_elements_of_clash_category_{0}</system:String>
    <system:String
        x:Key="Filters.NotCollisionCategories">BIM_clashes_not_clash_element_categories_{0}</system:String>
</ResourceDictionary>