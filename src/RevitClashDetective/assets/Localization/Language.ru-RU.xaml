<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String
        x:Key="Ok">OK</system:String>
    <system:String
        x:Key="Cancel">Cancel</system:String>

    <system:String
        x:Key="SettingsWindow.Title">Настройки</system:String>

    <system:String
        x:Key="ElementVisibilitySettings.ColorLabel">Цвет:</system:String>
    <system:String
        x:Key="ElementVisibilitySettings.TransparencyLabel">Прозрачность:</system:String>

    <system:String
        x:Key="SettingsWindow.Validation.TransparancyLessThanZero">Прозрачность не может быть меньше 0</system:String>
    <system:String
        x:Key="SettingsWindow.Validation.TransparancyGreaterThanHundred">Прозрачность не может быть больше 100%</system:String>
    <system:String
        x:Key="SettingsWindow.Validation.OffsetLessThanZero">Отступ не может быть меньше 0</system:String>
    <system:String
        x:Key="SettingsWindow.Validation.OffsetGreaterThan">Отступ не может быть больше {0}</system:String>
    <system:String
        x:Key="SettingsWindow.MainElementVisibilitySettings">Настройки видимости первого элемента коллизии</system:String>
    <system:String
        x:Key="SettingsWindow.SecondElementVisibilitySettings">Настройки видимости второго элемента коллизии</system:String>
    <system:String
        x:Key="SettingsWindow.SectionBoxSettings">Настройки 3D подрезки</system:String>
    <system:String
        x:Key="SettingsWindow.SectionBoxModeSettings">Режим 3D подрезки:</system:String>
    <system:String
        x:Key="SettingsWindow.SectionBoxAdditionalSize">Отступ подрезки, мм:</system:String>

    <system:String
        x:Key="Transactions.IsolateClashElements">Изоляция элементов коллизии</system:String>
    <system:String
        x:Key="Transactions.ClashGraphicSettings">Настройка графики элементов коллизии</system:String>
    <system:String
        x:Key="Transactions.ClearFilters">Сброс фильтров элементов коллизии</system:String>

    <system:String
        x:Key="Filters.FirstElementFilter">BIM_коллизии_первый_элемент_{0}</system:String>
    <system:String
        x:Key="Filters.SecondElementFilter">BIM_коллизии_второй_элемент_{0}</system:String>
    <system:String
        x:Key="Filters.NotFirstElementFilter">BIM_коллизии_не_первый_элемент_{0}</system:String>
    <system:String
        x:Key="Filters.NotSecondElementFilter">BIM_коллизии_не_второй_элемент_{0}</system:String>
    <system:String
        x:Key="Filters.NotElementsOfCollisionCategories">BIM_коллизии_не_элементы_категории_коллизии_{0}</system:String>
    <system:String
        x:Key="Filters.NotCollisionCategories">BIM_коллизии_не_категории_элементов_коллизии_{0}</system:String>

    <system:String
        x:Key="SectionBoxMode.AroundCollision">По месту пересечения</system:String>
    <system:String
        x:Key="SectionBoxMode.AroundElements">По конфликтующим элементам</system:String>
</ResourceDictionary>