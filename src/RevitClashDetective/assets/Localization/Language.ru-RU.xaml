<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String
        x:Key="Ok">OK</system:String>
    <system:String
        x:Key="Cancel">Cancel</system:String>

    <system:String
        x:Key="SettingsWindow.Title">Настройки</system:String>

    <system:String
        x:Key="ElementVisibilitySettings.ColorLabel">Цвет:</system:String>
    <system:String
        x:Key="ElementVisibilitySettings.TransparencyLabel">Прозрачность:</system:String>

    <system:String
        x:Key="SettingsWindow.Validation.TransparancyLessThanZero">Прозрачность не может быть меньше 0</system:String>
    <system:String
        x:Key="SettingsWindow.Validation.TransparancyGreaterThanHundred">Прозрачность не может быть больше 100%</system:String>
    <system:String
        x:Key="SettingsWindow.MainElementVisibilitySettings">Настройки видимости первого элемента коллизии</system:String>
    <system:String
        x:Key="SettingsWindow.SecondElementVisibilitySettings">Настройки видимости второго элемента коллизии</system:String>
</ResourceDictionary>