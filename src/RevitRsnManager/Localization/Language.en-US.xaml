<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Пример перевода на английский американский язык (google translate) -->

    <system:String x:Key="MainWindow.Title">Sample command</system:String>
    <system:String x:Key="MainWindow.Hello">Hello Revit!</system:String>
    <system:String x:Key="MainWindow.HelloCheck">Enter a value for the property to save.</system:String>

    <system:String x:Key="MainWindow.ButtonOk">OK</system:String>
    <system:String x:Key="MainWindow.ButtonCancel">Cancel</system:String>
    <system:String x:Key="MainWindow.UpdateSuccess">RSN.ini updated</system:String>
    <system:String x:Key="MainWindow.UpdateSuccessTitle">Done</system:String>
    <system:String x:Key="MainWindow.ConfigNotFound">Configurator not found,{x0}please create a new one{x1}in the editor</system:String>
    <system:String x:Key="MainWindow.ConfigNotFoundTitle">Error</system:String>
    <system:String x:Key="MainWindow.Empty">Server name cannot be empty.</system:String>
    <system:String x:Key="MainWindow.AlreadyExists">Server is already added.</system:String>
    <system:String x:Key="RsnConfigService.EmptyServerList">Server list is empty or not specified.</system:String>
    <system:String x:Key="RsnConfigService.RevitIniNotFound">Revit.ini file not found.</system:String>
    <system:String x:Key="RsnConfigService.ProjectPathMissing">Failed to get ProjectPath from Revit.ini.</system:String>
    <system:String x:Key="MainWindow.ErrorEmptyServerList">Please add at least one server.</system:String>
    <system:String x:Key="MainWindow.AutoConfigure">Auto configure</system:String>
    <system:String x:Key="MainWindow.AddServer">Add</system:String>

    <system:String x:Key="MainWindow.ConfirmTitle">Delete Confirmation</system:String>
    <system:String x:Key="MainWindow.ConfirmDeleteServer">Are you sure you want to delete the selected server?</system:String>

</ResourceDictionary>
